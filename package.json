{
  "name": "cssman-cli",
  "version": "0.1.0",
  "description": "A library intended to generate design tokens from a style source",
  "main": "src/index.js",
  "bin": {
    "cssman-cli": "src/cli.js"
  },
  "scripts": {
    "test": "c8 node --test",
    "lint": "biome lint . && markdownlint --config .github/configs/.markdownlintrc.json \"**/*.md\"",
    "format": "biome format --write . && markdownlint --config .github/configs/.markdownlint-fix.json --fix \"**/*.md\""
  },
  "keywords": [
    "design-tokens",
    "cli",
    "tokens",
    "style-management"
  ],
