  "repository": {
    "type": "git",
    "url": "git+https://github.com/USERNAME/REPOSITORY.git"
  },
  "bugs": {
    "url": "https://github.com/USERNAME/REPOSITORY/issues"
  },
  "homepage": "https://github.com/USERNAME/REPOSITORY#readme",
  "dependencies": {
    "axios": "^1.0.0"
  },
  "devDependencies": {
    "commander": "14.0.0",
    "css-tree": "3.1.0",
    "eslint": "9.28.0",
    "c8": "^7.11.0",
    "@biomejs/biome": "^1.7.3",
    "markdownlint-cli": "^0.41.0"
  }
